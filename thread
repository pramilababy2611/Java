class a extends Thread
{
public void run()
{
for(int i=0;i<=5;i++)
{
System.out.println(i);
}
}
}
class b extends Thread
{
public void run()
{
for(int j=0;j<=5;j++)
{
System.out.println(j);
}
}
}
class c extends Thread
{
public void run()
{
for(int k=0;k<=5;k++)
{
System.out.println(k);
}
}
}

class thr
{
public static void main(String args[])
{
a A=new a();
A.start();
b B=new b();
B.start();
c C=new c();
C.start();
}
}
