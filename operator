import java.util.Scanner;
class operator
{
public static void main(String args[])
{
int x,y,z;
Scanner in=new Scanner(System.in);
x=in.nextInt();
y=in.nextInt();
z=in.nextInt();
x++;
y--;
z=x+y;
y=z--+x;
x=x-(--z);
z+=y;
x-=x;
x+=y;
System.out.println(x);
System.out.println(y);
System.out.println(z);
}
}
